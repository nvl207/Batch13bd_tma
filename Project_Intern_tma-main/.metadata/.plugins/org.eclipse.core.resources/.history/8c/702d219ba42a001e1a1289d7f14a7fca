CREATE DATABASE CHECK_PROJECT
use CHECK_PROJECT
CREATE TABLE KhachHang (
MaKhachHang INT PRIMARY KEY,
TenKhachHang VARCHAR(100),
SoDienThoai VARCHAR(20),
DiaChiEmail VARCHAR(50)
);

CREATE TABLE ToaNha(
MaToaNha INT PRIMARY KEY,
TenToaNha VARCHAR(100),
DiaChi VARCHAR(200),
SoTang INT
);

CREATE TABLE Phong (
MaPhong INT PRIMARY KEY,
TenPhong VARCHAR(100),
MaToaNha INT,
FOREIGN KEY (MaToaNha) REFERENCES ToaNha(MaToaNha)
);